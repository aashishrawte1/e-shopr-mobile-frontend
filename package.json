{
  "name": "e-shopr-mobile-app",
  "version": "0.0.1",
  "description": "Frontend Bit",
  "author": "Aashish Rawte",
  "license": "Closed",
  "scripts": {
    "ng": "ng",
    "prestart": "npm install",
    "start": "ng --hmr=true --disableHostCheck=true --port=4300 --source-map=true serve",
    "postbuild": "mkdir www/sourceMapInspector/sourceMaps; mv www/sourceMapInspector/*.map www/sourceMapInspector/sourceMaps",
    "ionic": "ionic",
    "cordova": "cordova",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "postinstall": "npm run prettier -s",
    "serve-uat": "ng serve --configuration=uat --port=4200 --open=true --ssl=true",
    "serve-live": "ng serve --configuration=live --port=4200 --open=true --ssl=true",
    "create-git-tag": "ts-node --project=tsconfig.commonjs.json create-git-tag.ts",
    "build-stats": "ng build --stats-json --configuration=live",
    "create-bundle-stat-file": "webpack-bundle-analyzer www/stats-es2015.json",
    "show-bundle-map": "npm run build-stats && npm run create-bundle-stat-file",
    "build-local": "ng build",
    "lint-and-check": "npm run prettier && npm run prettier:check",
    "build:uat": "npm run lint-and-check && ng build --configuration=uat",
    "deploy:uat": "npm run build:uat -- --output-path=dist/uat && firebase deploy -P uat --only=hosting:uat_site",
    "build:live": "npm run lint-and-check && ng build --configuration=live",
    "deploy:live": "npm run build:live -- --output-path=dist/production && firebase deploy -P live --only=hosting:prod_site",
    "start:emulators": "npm run build:uat && npm run build:live && firebase emulators:start",
    "prepare:app:uat": "npm run build:uat && ts-node --project=tsconfig.commonjs.json scripts/prepare-env.ts uat && cap sync",
    "prepare:app:live": "npm run build:live && ts-node --project=tsconfig.commonjs.json scripts/prepare-env.ts live && cap sync"
  },
  "repository": {
    "type": "git",
